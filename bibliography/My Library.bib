	keywords = {Bandwidth, clocks, cross traffic, Delays, end-to-end detection, Entropy, inter-arrival times, intermediate nodes, Internet, IP-level compression, link-layer compression, Payloads, Probes, probing technique, Receivers, routers, synchronized clocks, telecommunication network routing, Telecommunication traffic, telecommunication traffic, traffic flows},
	pages = {1--8},
	file = {IEEE Xplore Abstract Record:/home/paul/.mozilla/firefox/mwad0hks.default/zotero/storage/TAKG3KXG/abs_all.html:text/html;IEEE Xplore Full Text PDF:/home/paul/.mozilla/firefox/mwad0hks.default/zotero/storage/IVPDUGTU/Pournaghshband et al. - 2014 - End-to-end detection of compression of traffic flo.pdf:application/pdf}
