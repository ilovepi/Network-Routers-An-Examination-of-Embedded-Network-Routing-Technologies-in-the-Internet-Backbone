	keywords = {application-agnostic method, Bandwidth, ChkDiff tool design, Delays, Internet, Internet access, IP networks, network neutrality, Payloads, Probes, Statistical analysis, target router, telecommunication traffic, traffic differentiation detection, traffic flow delay},
