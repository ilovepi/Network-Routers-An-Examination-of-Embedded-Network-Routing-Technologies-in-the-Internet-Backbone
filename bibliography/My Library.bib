	booktitle = {{CHI} '11 {Extended} {Abstracts} on {Human} {Factors} in {Computing} {Systems}},
	publisher = {ACM},
	author = {Rotman, Dana and Vieweg, Sarah and Yardi, Sarita and Chi, Ed and Preece, Jenny and Shneiderman, Ben and Pirolli, Peter and Glaisyer, Tom},
	year = {2011},
	keywords = {activism, change, design, participation, slacktivism, social media},
	pages = {819--822},
