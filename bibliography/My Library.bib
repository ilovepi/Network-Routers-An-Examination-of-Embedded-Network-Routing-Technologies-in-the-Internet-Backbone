	month = apr,
	year = {2009},
	keywords = {bit rate 100 Gbit/s, board footprint, Communications Society, core router line card, distributed-and-load balanced bloom filter, Energy consumption, field programmable gate arrays, Filters, FPGA, Hardware, hardware parallel architecture, Inspection, Internet, IP networks, IPv6 lookup, on-chip memory, parallel architectures, power dissipation, Random access memory, resource allocation, resource consumption, table lookup, telecommunication network routing, Throughput, Transceivers, Transport protocols, USA Councils},
	pages = {2518--2526},
	file = {IEEE Xplore Abstract Record:/home/paul/.mozilla/firefox/mwad0hks.default/zotero/storage/8AI8C92N/5062180.html:text/html;IEEE Xplore Full Text PDF:/home/paul/.mozilla/firefox/mwad0hks.default/zotero/storage/TWMZ3NFU/Song et al. - 2009 - IPv6 Lookups using Distributed and Load Balanced B.pdf:application/pdf}
