	pages = {785--795 vol.2},
	file = {IEEE Xplore Abstract Record:/home/paul/.mozilla/firefox/mwad0hks.default/zotero/storage/UW8ID6E2/Wang and Singh - 2004 - Computational energy cost of TCP.html:text/html;IEEE Xplore Full Text PDF:/home/paul/.mozilla/firefox/mwad0hks.default/zotero/storage/V7I8VCIQ/Wang and Singh - 2004 - Computational energy cost of TCP.pdf:application/pdf}
